# API settings
# Set to 'true' to enable external API calls (default: false)
VITE_ENABLE_EXTERNAL_APIS=false

# Logging settings
# Set to 'true' to enable console logging (default: true in development, false in production)
VITE_ENABLE_LOGGING=false

# Security settings
# Comma-separated list of allowed external domains
VITE_ALLOWED_DOMAINS=cdn.simpleicons.org,images.unsplash.com

# Feature flags
# Set to 'true' to enable analytics (default: true in production, false in development)
VITE_ENABLE_ANALYTICS=false 